ut type=\"text\" v-model=\"toEditBio\" placeholder=\"bio\"> <br><br>\n\n            </div>\n            \n\n            <br><br>\n            <button @click=\"memberEdit()\">click</button> \n\n            <br><br>\n        </div>\n\n        <div v-else-if=\"IfTokenIsYours == false\">\n            <AccessDenied />\n        </div>\n    </div>\n</template>\n\n<script>\nimport { ref } from 'vue';\nconst axios = require('axios').default;\n\nexport default {\n    props: [\"IfTokenIsYours\"],\n    setup() {\n        const toEditProfileImg = ref(\"\")\n        const toEditName = ref(\"\")\n        const toEditCompany = ref(\"\")\n        const toEditLocation = ref(\"\")\n        const toEditBio = ref(\"\")\n        \n        const gitID = ref(\"\")\n        const profileImg = ref(\"\")\n        const name = ref(\"\")\n        const company = ref(\"\")\n        const gitBlog = ref(\"\")\n        const location = ref(\"\")\n        const gitEmail = ref(\"\")\n        const bio = ref(\"\")\n        const updated_at = ref(\"\")\n\n        const AccessToken = localStorage.AccessToken\n\n        const nameLength = ref(0)\n        const profileImgLength = ref(0)\n        const companyLength = ref(0)\n        const locationLength = ref(0)\n        const bioLength = ref(0)\n\n        return {\n            gitID, profileImg, name, company, gitBlog, location, gitEmail, bio, updated_at,\n            toEditProfileImg, toEditName, toEditCompany, toEditLocation, toEditBio,\n            AccessToken,\n            nameLength, profileImgLength, companyLength, locationLength, bioLength,\n            }\n    }, \n    mounted() {\n        this.getMemberInfo()\n    },\n\n    methods: {\n        getMemberInfo () {\n            axios({\n                url: 'http://localhost:8080/getUserInfo/' + localStorage.gitID,\n                method: 'get',\n                headers: {\n                    'Authorization' : 'Bearer ' + this.AccessToken\n                }\n            }).then((response) => {\n                this.gitID = response.data.gitID\n                this.profileImg = response.data.profileImg\n                this.name = response.data.name\n                this.company = response.data.company\n                this.gitBlog = response.data.gitBlog\n                this.location = response.data.location\n                this.gitEmail = response.data.gitEmail\n                this.bio = response.data.bio\n                this.updated_at = response.data.updated_at\n\n                this.setDefaultInput()\n            })\n            \n        },\n        setDefaultInput() {\n            let margin = 3\n\n            this.toEditProfileImg = this.profileImg\n            this.toEditName = this.name\n            this.toEditCompany = this.company\n            this.toEditLocation = this.location\n            this.toEditBio = this.bio\n\n            this.profileImgLength = this.profileImg.length + margin \n            this.nameLength = this.toEditName.length + 1\n            this.companyLength = this.toEditCompany.length + 1\n            this.locationLength = this.toEditLocation.length + 1\n            this.bioLength = this.toEditLocation.length + 1\n        },\n        memberEdit() {\n            axios({\n                url: \"http://localhost:8080/memberEdit/\" + localStorage.gitID, \n                method: 'patch',\n                headers: {\n                    'Authorization' : 'Bearer ' + this.AccessToken\n                },\n                data: {\n                    profileImg : this.toEditProfileImg,\n                    name : this.toEditName,\n                    company : this.toEditCompany,\n                    location : this.toEditLocation,\n                    bio : this.toEditBio,\n                }\n            }).then((response) => {\n                if (response.status == 200) {\n                    this.getMemberInfo()\n                }\n            })\n        },\n    }\n}\n</script>\n\n<style scoped>\n    #info {\n        text-align: left;\n        margin-left: 20%;\n    }\n    input {\n        border: none;\n        background-color: #BEBEBE;\n        font-family: 'Courier New', Courier, monospace;\n    }\n    input:focus {\n        outline: none;\n    }\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}