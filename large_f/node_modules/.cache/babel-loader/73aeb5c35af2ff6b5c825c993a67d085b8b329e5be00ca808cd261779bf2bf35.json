.setDefaultInput();\n      });\n    },\n    setDefaultInput() {\n      let margin = 3;\n      this.toEditProfileImg = this.profileImg;\n      this.toEditName = this.name;\n      this.toEditCompany = this.company;\n      this.toEditLocation = this.location;\n      this.toEditBio = this.bio;\n      this.profileImgLength = this.profileImg.length + margin;\n      this.nameLength = this.toEditName.length + 1;\n      this.companyLength = this.toEditCompany.length + 1;\n      this.locationLength = this.toEditLocation.length + 1;\n      this.bioLength = this.toEditLocation.length + 1;\n    },\n    memberEdit() {\n      axios({\n        url: \"http://localhost:8080/memberEdit/\" + localStorage.gitID,\n        method: 'patch',\n        headers: {\n          'Authorization': 'Bearer ' + this.AccessToken\n        },\n        data: {\n          profileImg: this.toEditProfileImg,\n          name: this.toEditName,\n          company: this.toEditCompany,\n          location: this.toEditLocation,\n          bio: this.toEditBio\n        }\n      }).then(response => {\n        if (response.status == 200) {\n          this.getMemberInfo();\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAgDA,SAASA,GAAE,QAAS,KAAK;AACzB,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAO,CAAC,CAACC,OAAO;AAEtC,eAAe;EACXC,KAAK,EAAE,CAAC,gBAAgB,CAAC;EACzBC,KAAK,GAAG;IACJ,MAAMC,gBAAe,GAAIN,GAAG,CAAC,EAAE;IAC/B,MAAMO,UAAS,GAAIP,GAAG,CAAC,EAAE;IACzB,MAAMQ,aAAY,GAAIR,GAAG,CAAC,EAAE;IAC5B,MAAMS,cAAa,GAAIT,GAAG,CAAC,EAAE;IAC7B,MAAMU,SAAQ,GAAIV,GAAG,CAAC,EAAE;IAExB,MAAMW,KAAI,GAAIX,GAAG,CAAC,EAAE;IACpB,MAAMY,UAAS,GAAIZ,GAAG,CAAC,EAAE;IACzB,MAAMa,IAAG,GAAIb,GAAG,CAAC,EAAE;IACnB,MAAMc,OAAM,GAAId,GAAG,CAAC,EAAE;IACtB,MAAMe,OAAM,GAAIf,GAAG,CAAC,EAAE;IACtB,MAAMgB,QAAO,GAAIhB,GAAG,CAAC,EAAE;IACvB,MAAMiB,QAAO,GAAIjB,GAAG,CAAC,EAAE;IACvB,MAAMkB,GAAE,GAAIlB,GAAG,CAAC,EAAE;IAClB,MAAMmB,UAAS,GAAInB,GAAG,CAAC,EAAE;IAEzB,MAAMoB,WAAU,GAAIC,YAAY,CAACD,WAAU;IAE3C,MAAME,UAAS,GAAItB,GAAG,CAAC,CAAC;IACxB,MAAMuB,gBAAe,GAAIvB,GAAG,