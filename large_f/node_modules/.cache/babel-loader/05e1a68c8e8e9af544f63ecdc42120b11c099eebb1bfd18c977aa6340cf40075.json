{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from \"@vue/reactivity\";\nconst axios = require('axios').default;\nexport default {\n  setup() {\n    const templates = ref([]);\n    return {\n      templates\n    };\n  },\n  mounted() {\n    var keys = {\n      a: false,\n      s: false\n    };\n    this.getAllTemplates();\n    var textarea = document.getElementById(\"textarea\");\n    // var textarea = document.textarea\n    addEventListener('keydown', e => {\n      if (e.key == 'a') {\n        keys.a = true;\n      } else if (e.key == 'command') {\n        keys.s = true;\n      } else {\n        keys.a = false;\n        keys.s = false;\n      }\n      if (keys.a == true && keys.s == true) {\n        setTimeout(function () {\n          textarea.value = textarea.value + \"NODE\";\n        }, 100);\n        keys.a = false;\n        keys.s = false;\n      }\n    });\n  },\n  methods: {\n    getAllTemplates() {\n      axios({\n        url: 'http://localhost:8080/template/' + localStorage.getItem(\"gitID\"),\n        method: 'get',\n        headers: {\n          'Authorization': 'Bearer ' + localStorage.getItem(\"AccessToken\")\n        }\n      }).then(response => {\n        response.data.forEach(element => {\n          this.templates.push(element);\n        });\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAmBA,SAASA,GAAE,QAAS,iBAAgB;AACpC,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAO,CAAC,CAACC,OAAO;AAEtC,eAAgB;EACZC,KAAK,GAAG;IACJ,MAAMC,SAAQ,GAAIL,GAAG,CAAC,EAAE;IAExB,OAAO;MAAEK;IAAU;EACvB,CAAC;EACDC,OAAO,GAAG;IACN,IAAIC,IAAG,GAAI;MACPC,CAAC,EAAE,KAAK;MACRC,CAAC,EAAE;IACP;IAEA,IAAI,CAACC,eAAe,EAAC;IACrB,IAAIC,QAAO,GAAIC,QAAQ,CAACC,cAAc,CAAC,UAAU;IACjD;IACAC,gBAAgB,CAAC,SAAS,EAAGC,CAAC,IAAK;MAC/B,IAAIA,CAAC,CAACC,GAAE,IAAK,GAAG,EAAE;QACdT,IAAI,CAACC,IAAI,IAAG;MAChB,OACK,IAAIO,CAAC,CAACC,GAAE,IAAK,SAAS,EAAE;QACzBT,IAAI,CAACE,IAAI,IAAG;MAChB,OACK;QACDF,IAAI,CAACC,IAAI,KAAI;QACbD,IAAI,CAACE,IAAI,KAAI;MACjB;MAEA,IAAIF,IAAI,CAACC,KAAK,IA