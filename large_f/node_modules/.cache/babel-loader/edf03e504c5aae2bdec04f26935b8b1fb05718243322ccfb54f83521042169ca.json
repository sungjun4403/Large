{"ast":null,"code":"const axios = require('axios').default;\nexport default {\n  mounted() {\n    this.passCode();\n  },\n  methods: {\n    passCode() {\n      const url = window.location.href;\n      const code = url.split(\"=\")[1];\n      axios.get(\"https://api.large-devlog.com/auth/github/callback?code=\" + code).then(response => {\n        if (response.status == 200) {\n          //로그인 성공시 홈으로 리다이렉트                    \n          localStorage.setItem(\"AccessToken\", response.headers[\"accesstoken\"]);\n          localStorage.setItem(\"memberToken\", response.headers[\"membertoken\"]);\n          localStorage.setItem(\"memberToken\", response.headers[\"membertoken\"]);\n          localStorage.setItem(\"gitID\", response.headers[\"gitid\"]);\n          window.location.href = \"https://large-devlog.com/\" + response.headers[\"gitid\"];\n        } else {\n          //로그인 실패시 LoginFailed로 리다이렉트 \n          window.location.href = \"https://large-devlog.com/login_failed\";\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AASA,MAAMA,KAAI,GAAIC,OAAO,CAAC,OAAO,CAAC,CAACC,OAAO;AAEtC,eAAe;EACXC,OAAO,GAAG;IACN,IAAI,CAACC,QAAQ,EAAC;EAClB,CAAC;EAEDC,OAAO,EAAE;IACLD,QAAQ,GAAG;MACP,MAAME,GAAE,GAAIC,MAAM,CAACC,QAAQ,CAACC,IAAG;MAC/B,MAAMC,IAAG,GAAIJ,GAAG,CAACK,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC;MAC7BX,KAAK,CAACY,GAAG,CAAC,yDAAwD,GAAIF,IAAI,CAAC,CAACG,IAAI,CAAEC,QAAQ,IAAK;QAC3F,IAAIA,QAAQ,CAACC,MAAK,IAAK,GAAG,EAAE;UAAE;UAC1BC,YAAY,CAACC,OAAO,CAAC,aAAa,EAAEH,QAAQ,CAACI,OAAO,CAAC,aAAa,CAAC;UACnEF,YAAY,CAACC,OAAO,CAAC,aAAa,EAAEH,QAAQ,CAACI,OAAO,CAAC,aAAa,CAAC;UACnEF,YAAY,CAACC,OAAO,CAAC,aAAa,EAAEH,QAAQ,CAACI,OAAO,CAAC,aAAa,CAAC;UACnEF,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,QAAQ,CAACI,OAAO,CAAC,OAAO,CAAC;UAGvDX,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAI,2BAA0B,GAAIK,QAAQ,CAACI,OAAO,CAAC,OAAO;QACjF,OAEK;UAAE;UACHX,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAI,uCAAsC;QACjE;MACJ,CAAC;IACL;EACJ;AACJ","names":["axios","require","default","mounted","passCode","methods","url","window","location","href","code","split","get","then","response","status","localStorage","setItem","headers"],"sourceRoot":"","sources":["/Users/sungjun/Documents/Large/large_f/src/components/Login/AfterLogin.vue"],"sourcesContent":["<template>\n    <div id=\"body\">\n\n        <div id=\"txt\"><h2>REDIRECTING</h2> to <a href=\"/\">Home</a></div>\n\n    </div>\n</template>\n\n<script>\nconst axios = require('axios').default;\n\nexport default {\n    mounted() {\n        this.passCode()\n    },\n\n    methods: {\n        passCode() {\n            const url = window.location.href\n            const code = url.split(\"=\")[1]\n            axios.get(\"https://api.large-devlog.com/auth/github/callback?code=\" + code).then((response) => {\n                if (response.status == 200) { //로그인 성공시 홈으로 리다이렉트                    \n                    localStorage.setItem(\"AccessToken\", response.headers[\"accesstoken\"])\n                    localStorage.setItem(\"memberToken\", response.headers[\"membertoken\"])\n                    localStorage.setItem(\"memberToken\", response.headers[\"membertoken\"])\n                    localStorage.setItem(\"gitID\", response.headers[\"gitid\"])\n\n                    \n                    window.location.href = \"https://large-devlog.com/\" + response.headers[\"gitid\"]\n                }\n                \n                else { //로그인 실패시 LoginFailed로 리다이렉트 \n                    window.location.href = \"https://large-devlog.com/login_failed\"\n                }\n            }) \n        },\n    }\n}\n</script>\n\n<style>\n#txt {\n    \n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}