).default\nexport default {\n    props: [\"IfTokenIsYours\"],\n    setup() {\n        const gitID = localStorage.gitID\n        return { gitID }    \n        \n    },\n    methods: {\n        DeleteAccount() {\n            axios({\n                url: 'http://localhost:8080/deleteNCascade/' + this.gitID,\n                method: 'get',\n                headers: {\n                    'Authorization' : 'Bearer ' + localStorage.a\n                }\n            }).then(()=> {\n                window.location.href = \"http://localhost:3000/\"\n            })\n        },\n    }\n\n}\n</script>\n\n<style scoped>\n    #PreferencesBody {\n        text-align: center;\n    }\n    #span1 {\n        \n        font-size: larger;\n    }\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}