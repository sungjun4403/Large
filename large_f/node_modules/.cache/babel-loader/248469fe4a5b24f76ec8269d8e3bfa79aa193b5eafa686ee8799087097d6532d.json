{"ast":null,"code":"import { ref } from \"@vue/reactivity\";\nconst axios = require('axios').default;\nexport default {\n  setup() {\n    const title = ref(\"\");\n    const body = ref(\"\");\n    const ifAds = ref(false);\n    const AccessToken = ref(\"\");\n    return {\n      title,\n      body,\n      ifAds,\n      AccessToken\n    };\n  },\n  beforeMount() {\n    this.AccessToken = localStorage.AccessToken;\n  },\n  methods: {\n    PostCreate() {\n      axios({\n        url: 'https://api.large-devlog.com/post',\n        method: 'post',\n        headers: {\n          'Authorization': 'Bearer ' + this.AccessToken\n        },\n        data: {\n          title: this.title,\n          body: this.body,\n          ifAds: this.getCheckValueIfAds()\n        }\n      });\n    },\n    getCheckValueIfAds() {\n      const ifAds = document.getElementById('ifAds');\n      if (ifAds.checked) {\n        return true;\n      } else {\n        return false;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAgCA,SAASA,GAAE,QAAS,iBAAiB;AAErC,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAO,CAAC,CAACC,OAAO;AAEtC,eAAgB;EACZC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,GAAG,CAAC,EAAE;IACpB,MAAMM,IAAG,GAAIN,GAAG,CAAC,EAAE;IACnB,MAAMO,KAAI,GAAIP,GAAG,CAAC,KAAK;IACvB,MAAMQ,WAAU,GAAIR,GAAG,CAAC,EAAE;IAE1B,OAAO;MAACK,KAAK;MAAEC,IAAI;MAAEC,KAAK;MAAEC;IAAW;EAC3C,CAAC;EACDC,WAAW,GAAG;IACV,IAAI,CAACD,WAAU,GAAIE,YAAY,CAACF,WAAU;EAC9C,CAAC;EACDG,OAAO,EAAE;IACLC,UAAU,GAAG;MACTX,KAAK,CAAC;QACFY,GAAG,EAAE,mCAAmC;QACxCC,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UACL,eAAc,EAAI,SAAQ,GAAI,IAAI,CAACP;QACvC,CAAC;QACDQ,IAAI,EAAE;UACFX,KAAI,EAAI,IAAI,CAACA,KAAK;UAClBC,IAAG,EAAI,IAAI,CAACA,IAAI;UAChBC,KAAI,EAAI,IAAI,CAACU,kBAAkB;QACnC;MACJ,CAAC;IACL,CAAC;IACDA,kBAAkB,GAAG;MACjB,MAAMV,KAAI,GAAIW,QAAQ,CAACC,cAAc,CAAC,OAAO;MAC7C,IAAIZ,KAAK,CAACa,OAAO,EAAE;QACf,OAAO,IAAG;MACd,OACK;QACD,OAAO,KAAI;MACf;IACJ;EACJ;AACJ,CAAC","names":["ref","axios","require","default","setup","title","body","ifAds","AccessToken","beforeMount","localStorage","method