{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from \"@vue/reactivity\";\nconst axios = require('axios').default;\nexport default {\n  setup() {\n    const templates = ref([]);\n    return {\n      templates\n    };\n  },\n  mounted() {\n    this.getAllTemplates();\n    // var textarea = document.getElementById(\"textarea\")\n    var textarea = document.getElementById(\"textarea\");\n    textarea.addEventListener('keydown', e => {\n      if (e.key == ';') {\n        textarea.value = textarea.value + \"NODE\";\n      }\n    });\n  },\n  methods: {\n    getAllTemplates() {\n      axios({\n        url: 'http://localhost:8080/template/' + localStorage.getItem(\"gitID\"),\n        method: 'get',\n        headers: {\n          'Authorization': 'Bearer ' + localStorage.getItem(\"AccessToken\")\n        }\n      }).then(response => {\n        response.data.forEach(element => {\n          this.templates.push(element);\n        });\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAmBA,SAASA,GAAE,QAAS,iBAAgB;AACpC,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAO,CAAC,CAACC,OAAO;AAEtC,eAAgB;EACZC,KAAK,GAAG;IACJ,MAAMC,SAAQ,GAAIL,GAAG,CAAC,EAAE;IAExB,OAAO;MAAEK;IAAU;EACvB,CAAC;EACDC,OAAO,GAAG;IACN,IAAI,CAACC,eAAe,EAAC;IACrB;IACA,IAAIC,QAAO,GAAIC,QAAQ,CAACC,cAAc,CAAC,UAAU;IACjDF,QAAQ,CAACG,gBAAgB,CAAC,SAAS,EAAGC,CAAC,IAAK;MACxC,IAAIA,CAAC,CAACC,GAAE,IAAK,GAAG,EAAE;QACdL,QAAQ,CAACM,KAAI,GAAIN,QAAQ,CAACM,KAAI,GAAI,MAAK;MAC3C;IACJ,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLR,eAAe,GAAG;MACdN,KAAK,CAAC;QACFe,GAAG,EAAE,iCAAgC,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QACtEC,MAAM,EAAE,KAAK;QACbC,OAAO,EAAE;UACL,eAAc,EAAI,SAAQ,GAAIH,YAAY,CAACC,OAAO,CAAC,aAAa;QACpE;MACJ,CAAC,CAAC,CAACG,IAAI,CAAEC,QAAQ,IAAK;QAClBA,QAAQ,CAACC,IAAI,CAACC,OAAO,CAACC,OAAM,IAAK;UAC7B,IAAI,CAACpB,SAAS,CAACqB,IAAI,CAACD,OAAO;QAC/B,CAAC,CAAC;MACN,CAAC;IACL;EAEJ;AACJ,CAAC","names":["ref","axios","require","default","setup","templates","mounted","getAllTemplates","textarea","document","getElementById","addEventListener","e","key","value","methods","url","localStorage","getItem","method","headers","then","response","data","forEach","element","push"],"sourceRoot":"","sources":["/Users/sungjun/Documents/Large/large_f/src/components/NavBar/Preferences/Template/Template.vue"],"sourcesContent":["<template>\n    <div id=\"TemplateBody\">\n        <br>\n        Template\n\n        <li v-for=\"template in templates\" :key=\"template.id\">\n            {{template.templateNumber}}\n            {{template.templateName}}\n            {{template.template}}\n            {{template.hotKey}}\n            <br>\n        </li>\n\n        <br>\n        <textarea id=\"textarea\" cols=\"30\" rows=\"10\"></textarea>\n    </div>\n</template>\n\n<script>\nimport { ref } from \"@vue/reactivity\"\nconst axios = require('axios').default;\n\nexport default ({\n    setup() {\n        const templates = ref([])\n\n        return { templates }\n    },\n    mounted() {\n        this.getAllTemplates()\n        // var textarea = document.getElementById(\"textarea\")\n        var textarea = document.getElementById(\"textarea\")\n        textarea.addEventListener('keydown', (e) => {\n            if (e.key == ';') {\n                textarea.value = textarea.value + \"NODE\"\n            }\n        })\n    },\n    methods: {\n        getAllTemplates() {\n            axios({\n                url: 'http://localhost:8080/template/' + localStorage.getItem(\"gitID\"),\n                method: 'get',\n                headers: {\n                    'Authorization' : 'Bearer ' + localStorage.getItem(\"AccessToken\")\n                }\n            }).then((response) => {\n                response.data.forEach(element => {\n                    this.templates.push(element)\n                });\n            })\n        }\n            \n    }\n})\n</script>\n\n<style>\n    #TemplateBody {\n        text-align: center;\n    }\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}