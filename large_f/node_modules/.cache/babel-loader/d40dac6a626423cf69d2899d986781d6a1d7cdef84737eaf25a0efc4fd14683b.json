{"ast":null,"code":"import { ref } from \"@vue/reactivity\";\nconst axios = require('axios').default;\nexport default {\n  setup() {\n    const title = ref(\"\");\n    const body = ref(\"\");\n    const ifAds = ref(false);\n    return {\n      title,\n      body,\n      ifAds\n    };\n  },\n  created() {\n    console.log(\"CREATED\");\n  },\n  mounted(a) {\n    console.log(\"MOUNTED\");\n  },\n  beforeMount() {\n    console.log(\"BEFORE MOUNT\");\n  },\n  methods: {\n    PostCreate() {\n      axios.post(\"http://localhost:8080/post\", {\n        title: this.title,\n        body: this.body,\n        ifAds: this.getCheckValueIfAds()\n      });\n    },\n    getCheckValueIfAds() {\n      const ifAds = document.getElementById('ifAds');\n      if (ifAds.checked) {\n        return true;\n      } else {\n        return false;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAuBA,SAASA,GAAE,QAAS,iBAAiB;AAErC,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAO,CAAC,CAACC,OAAO;AAGtC,eAAgB;EACZC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,GAAG,CAAC,EAAE;IACpB,MAAMM,IAAG,GAAIN,GAAG,CAAC,EAAE;IACnB,MAAMO,KAAI,GAAIP,GAAG,CAAC,KAAK;IAEvB,OAAO;MAACK,KAAK;MAAEC,IAAI;MAAEC;IAAK;EAC9B,CAAC;EACDC,OAAO,GAAG;IACNC,OAAO,CAACC,GAAG,CAAC,SAAS;EACzB,CAAC;EACDC,OAAO,CAACC,CAAC,EAAE;IACPH,OAAO,CAACC,GAAG,CAAC,SAAS;EACzB,CAAC;EACDG,WAAW,GAAG;IACVJ,OAAO,CAACC,GAAG,CAAC,cAAc;EAC9B,CAAC;EAEDI,OAAO,EAAE;IACLC,UAAU,GAAG;MACTd,KAAK,CAACe,IAAI,CAAC,4BAA4B,EAAE;QACrCX,KAAI,EAAI,IAAI,CAACA,KAAK;QAClBC,IAAG,EAAI,IAAI,CAACA,IAAI;QAChBC,KAAI,EAAI,IAAI,CAACU,kBAAkB;MACnC,CAAC;IACL,CAAC;IACDA,kBAAkB,GAAG;MACjB,MAAMV,KAAI,GAAIW,QAAQ,CAACC,cAAc,CAAC,OAAO;MAC7C,IAAIZ,KAAK,CAACa,OAAO,EAAE;QACf,OAAO,IAAG;MACd,OACK;QACD,OAAO,KAAI;MACf;IACJ;EACJ;AACJ,CAAC","names":["ref","axios","require","default","setup","title","body","ifAds","created","console","log","mounted","a","beforeMount","methods","PostCreate","post","getCheckValueIfAds","document","getElementById","checked"],"sourceRoot":"","sources":["/Users/sungjun/Documents/Large/large_f/src/components/Post/PostCreate.vue"],"sourcesContent":["<template>\n    <div id=\"body\">\n        <h2>CREATE POST</h2>\n\n        <br>\n\n        <input type=\"text\" id=\"title\" name=\"title\" v-model=\"title\" placeholder=\"title\">\n\n        <br><br>\n\n        <input type=\"text\" name=\"body\" v-model=\"body\" placeholder=\"body\">\n\n        <br><br>\n\n        ifAds<input type=\"checkbox\" id=\"ifAds\" name=\"ifAds\" v-model=\"ifAds\">\n\n        <br><br>\n        \n        <button @click=\"PostCreate()\">click</button> \n    </div>\n</template>\n\n<script>\nimport { ref } from \"@vue/reactivity\";\n\nconst axios = require('axios').default;\n\n\nexport default ({\n    setup() {\n        const title = ref(\"\")\n        const body = ref(\"\")\n        const ifAds = ref(false)\n\n        return {title, body, ifAds}\n    },\n    created() {\n        console.log(\"CREATED\")\n    },\n    mounted(a) {\n        console.log(\"MOUNTED\")\n    },\n    beforeMount() {\n        console.log(\"BEFORE MOUNT\")\n    },\n\n    methods: {\n        PostCreate() {\n            axios.post(\"http://localhost:8080/post\", {\n                title : this.title,\n                body : this.body,\n                ifAds : this.getCheckValueIfAds(),\n            })\n        },\n        getCheckValueIfAds() {\n            const ifAds = document.getElementById('ifAds')\n            if (ifAds.checked) {\n                return true\n            }\n            else {\n                return false\n            }\n        },\n    },\n});\n</script>\n\n<style>\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}