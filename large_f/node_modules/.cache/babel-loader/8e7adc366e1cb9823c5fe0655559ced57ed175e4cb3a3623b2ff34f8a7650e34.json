{"ast":null,"code":"import { ref } from 'vue';\nconst axios = require('axios').default;\nexport default {\n  setup() {\n    const gitID = ref(\"\");\n    const profileImg = ref(\"\");\n    const name = ref(\"\");\n    const company = ref(\"\");\n    const gitBlog = ref(\"\");\n    const location = ref(\"\");\n    const gitEmail = ref(\"\");\n    const bio = ref(\"\");\n    const updated_at = ref(\"\");\n    return {\n      gitID,\n      profileImg,\n      name,\n      company,\n      gitBlog,\n      location,\n      gitEmail,\n      bio,\n      updated_at\n    };\n  },\n  mounted() {\n    this.getMemberInfo();\n  },\n  methods: {\n    getMemberInfo() {\n      axios.post(\"http://localhost:8080/\" + localStorage.getItem(\"gitID\") + \"/getUserInfo\", {\n        gitID: localStorage.getItem(\"gitID\"),\n        AccessToken: localStorage.getItem(\"AccessToken\")\n      }).then(response => {\n        console.log(response.data);\n      });\n    },\n    memberEdit() {\n      axios.patch(\"http://localhost:8080/post\", {\n        profileImg: this.profileImg.value,\n        name: this.name.value,\n        company: this.company.value,\n        location: this.location.value,\n        bio: this.bio.value\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAmCA,SAASA,GAAE,QAAS,KAAK;AACzB,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAO,CAAC,CAACC,OAAO;AAEtC,eAAe;EACXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,GAAG,CAAC,EAAE;IACpB,MAAMM,UAAS,GAAIN,GAAG,CAAC,EAAE;IACzB,MAAMO,IAAG,GAAIP,GAAG,CAAC,EAAE;IACnB,MAAMQ,OAAM,GAAIR,GAAG,CAAC,EAAE;IACtB,MAAMS,OAAM,GAAIT,GAAG,CAAC,EAAE;IACtB,MAAMU,QAAO,GAAIV,GAAG,CAAC,EAAE;IACvB,MAAMW,QAAO,GAAIX,GAAG,CAAC,EAAE;IACvB,MAAMY,GAAE,GAAIZ,GAAG,CAAC,EAAE;IAClB,MAAMa,UAAS,GAAIb,GAAG,CAAC,EAAE;IAEzB,OAAO;MAACK,KAAK;MAAEC,UAAU;MAAEC,IAAI;MAAEC,OAAO;MAAEC,OAAO;MAAEC,QAAQ;MAAEC,QAAQ;MAAEC,GAAG;MAAEC;IAAU;EAC1F,CAAC;EACDC,OAAO,GAAG;IACN,IAAI,CAACC,aAAa,EAAC;EACvB,CAAC;EAEDC,OAAO,EAAE;IACLD,aAAY,GAAK;MACbd,KAAK,CAACgB,IAAI,CAAC,wBAAuB,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,IAAI,cA